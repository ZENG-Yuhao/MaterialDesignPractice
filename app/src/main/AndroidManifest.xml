<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.esigelec.zengyuhao.materialdesignpractice">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_devices_black_48dp"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".CardViewActivity"/>
        <activity android:name=".ShadowAndClippingActivity"/>
        <activity
            android:name=".ActionBarActivity"
            android:label="@string/title_activity_action_bar"/>
        <activity android:name=".PaperTabStripActivity"/>
        <activity
            android:name=".NavigationDrawerActivity"
            android:label="DrawerLayout"
            android:logo="@drawable/ic_arrow_back_black_24dp"/>
        <activity android:name=".NotificationActivity"/>
        <activity android:name=".CapturingPhotoActivity"/>
        <activity android:name=".SnackerBarActivity"/>
        <activity android:name=".CustomViewActivity"/>
        <activity android:name=".IntentServiceActivity"/>

        <service
            android:name=".Service.MyIntentService"
            android:exported="false"/>

        <activity android:name=".RippleEffectActivity"/>
        <activity android:name=".CircularRevealActivity"/>
        <activity android:name=".SharedElementsTransitionActivity"/>
        <activity android:name=".SharedElementDetailsActivity"/>
        <activity android:name=".AlterDialogActivity"/>
        <activity android:name=".ObserverAndObservableActivity"/>
        <activity android:name=".MVP.View.LoginActivity"/>
        <activity android:name=".MVP.View.Login2Activity"/>
        <activity android:name=".MVP.View.Login3Activity">
        </activity>
    </application>

</manifest>